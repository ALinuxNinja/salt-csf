csf:
  host:
    common:
      service:
        csf: True
        lfd: True
    host1:
      service:
        csf: True
        lfd: False
      config:
        main:
          TESTING: 0
        ignore:
          - "10.0.0.0/8 #Internal Subnet"
      rule:
        post: {}
        pre:
          groups:
            - DOCKER
          contents:
            - /sbin/iptables...
  rule:
    DOCKER:
      - service docker restart
